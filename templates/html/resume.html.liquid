<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no, minimal-ui">
    <title>{{ resume.basics.name }}</title>
    <style>
      {{ style }}
    </style>
    <link href='https://fonts.googleapis.com/css?family=Roboto:300,500,700' rel='stylesheet' type='text/css'>
  </head>
  <body>
    <div class="container">
      <header>{% assign b = resume.basics %}
        <div class="item">
          <div class="left">
            <div class="item">
              <div class="left">
                <div class="logo">
                  <svg viewBox="0 0 500 500">
                    <path fill="#34bb6d" d="M426.3 72.7A250 250 0 0 1 491 314.2 250 250 0 0 1 314.2 491 250 250 0 0 1 72.7 426.3L249.5 249.5Z"></path>
                    <path fill="#1aa153" d="m143.2 356.8 140.4 140.4C401.3 480.3 482.8 394 497.8 279.1L359 140.3Z"></path>
                    <path fill="#27ae60" d="m73.2 426.8a250 250 0 0 1 0-353.6 250 250 0 0 1 353.6 0L250 250Z"></path>
                    <path fill="#d5d5d5" d="m142.9 141.5 0 216 215.9 0 0-216-107.7 108.2z"></path>
                    <path fill="#eaeaea" d="m251 249.4 0 107.6-108-108 0-107.7z"></path>
                    <path fill="#fff" d="m251.1 248.9 0 107.6 108-108 0-107.7z"></path>
                  </svg>
                </div>
              </div>
              <div class="right-header">
                <h1>{{ b.name }}</h1>
                <h2 class="title">{{ b.label }}</h2>
              </div>
            </div>
          </div>
          <div class="right">
            <section class="contact">
              <div class="email">
                <a href="mailto:{{ b.email }}">
                  {{ b.email }}
                  <div class="mini-icon">
                    <svg viewBox="0 0 685 685">
                      <path fill="#444" d="M0,342.5a342.5,342.5 0 1,0 685,0a342.5,342.5 0 1,0 -685,0"></path>
                      <path fill="#f6f6f6" d="m331.3 367.5 17.8 0 161.5-161.5-340 0zM507.9 478.6l-338.5 0 112.7-112.7c21.2 21.4 38.3 33 57.2 33 17.2 0 29.4-5.4 56.3-32.5zM259.8 341.4l-111.7-111.7 0 224.6zM417.3 341.4l111.7-111.7 0 224.6z"></path>
                    </svg>
                  </div>
                </a>
              </div>
              <div class="website">
                <a href="{{ b.website }}">
                  {{ b.website }}
                  <div class="mini-icon">
                    <svg viewBox="0 0 685 685">
                      <path fill="#444" d="M0,342.5a342.5,342.5 0 1,0 685,0a342.5,342.5 0 1,0 -685,0"></path>
                      <path fill="#f6f6f6" d="m246 467 73-81 61 141c13 25 52 4 38-24l-57-133 103 0-195-211c-3-3-23-9-24 9z"></path>
                    </svg>
                  </div>
                </a>
              </div>
              {% for p in b.profiles %}
              <div class="network">
                <a href="{{ p.url }}">
                  {{ p.username }}
                  <div class="mini-icon">
                  {% case p.network %}
                  {% when 'GitHub' %}
                  <svg viewBox="0 0 685 685">
                    <path fill="#444" d="M0,344a342,342 0 1,0 684,0a342,342 0 1,0 -684,0"></path>
                    <path fill="#f6f6f6" d="m289.2 431.3c-7.6 10.8-12.4 21.6-14.2 32.3-5.4 4.7-20.6 7.6-38.2 5.8-7-0.9-19-5.7-43.6-38.9-15.7-15.8-27-13.8-30.3-14-4.9 0.2-9.1 1.4-11.6 5.5 8.8 7.3 17.5 15.3 26.7 26 3.6 3.9 9.9 20.1 21.1 35.2 17.9 18.6 42.2 19.2 74.8 16.5 1.9 17.1 1.9 33.6 0 52.1 48.7 12.4 81.1 9.2 128.3 1.8l-0.7-85.4c-1-12.2-4.3-24.6-14.9-37.8 65.5-10.5 110.4-39.4 112.6-111.2 1.4-28.6-2.3-56.2-25.4-79.9 5.4-19.6 7.1-40.5-2.9-65.4-21.9-3.4-45 9.1-68.3 24.3-39.5-9.5-81.4-12.4-128.6 0.4-21-14.3-42.1-28.6-68.3-25.4-9.6 23.6-9.7 45.7-2.9 66.9-25.1 30.2-25.5 47.3-26.2 68.7 2.9 106.5 75.4 114.8 112.6 122.4z"></path>
                  </svg>
                  {% when 'LinkedIn' %}
                  <svg viewBox="0 0 685 685">
                    <path fill="#444" d="M0,342.5a342.5,342.5 0 1,0 685,0a342.5,342.5 0 1,0 -685,0"></path>
                    <path fill="#f6f6f6" d="M180.9,195.5a38.5,35.7 0 1,0 77,0a38.5,35.7 0 1,0 -77,0"></path>
                    <path fill="#f6f6f6" d="m 184.2,257.1 69,0 0,205.5 -69,0 z"></path>
                    <path fill="#f6f6f6" d="m288.9 462.3 0-205.5 69.5 0 0 25.1c29.9-43 98.8-37.5 121.3 0 16.1 26.6 15.7 37.8 17.4 62.9l0 118-69.5 0-0.8-115.7c-1-24.9-10.7-38.4-32.1-38.7-15.9 0.4-35.4 14.1-35.4 31l0 123.7z"></path>
                  </svg>
                  {% endcase %}
                  </div>
                </a>
              </div>
              {% endfor %}
              <div class="location">
                {{ b.location.city }}, {{ b.location.region }}, {{ b.location.countryCode }}
                <div class="mini-icon">
                  <svg viewBox="0 0 685 685">
                    <path fill="#444" d="M0 342.5a342.5 342.5 0 1 0 685 0 342.5 342.5 0 1 0-685 0"></path>
                    <path fill="#f6f6f6" d="m343.9 577.9c2.3 2.9 5.5 2.9 7.6 0l104-206.8c33.3-60.7 67.8-121.3 0-196.3-60.6-61.4-146.3-70.7-216 0-63.1 72.8-39.8 133.6 0 200z"></path>
                    <path fill="#444" d="M291.7,273.6a56.9,55.9 0 1,0 113.8,0a56.9,55.9 0 1,0 -113.8,0"></path>
                  </svg>
                </div>
              </div>
            </section>
          </div>
        </div>
      </header>
      <section class="skills">
        <h2>Technical Skills</h2>
        {% for s in resume.skills %}
        <div class="item">
          <div class="left">
            {{ s.name }}:
          </div>
          <div class="right">
            <strong>
              {{ s.keywords | join: ", "}}
            </strong>
          </div>
        </div>
        {% endfor %}
      </section>
      <section class="summary">
        <h2>Summary</h2>
        <p>{{ resume.basics.summary }}</p>
      </section>
      <section class="education">
        <h2>Education</h2>
        {% for e in resume.education %}
        <div class="item">
          <div class="left">
            <div class="institution">
              <strong>
                {{ e.institution }}
              </strong>
            </div>
            <div class="studyType">
              {{ e.studyType }}
            </div>
          </div>
          <div class="right">
            <div class="area">
              <strong>
                {{ e.area }}
              </strong>
            </div>
            <div class="date">
              <span class="startDate">
                {{ e.startDate | date: "%b %Y" }}
              </span>
              {% if e.endDate %}
              <span class="endDate">
                - {{ e.endDate | date: "%b %Y" }}
              </span>
              {% else %}
              <span class="endDate">
                - Present
              </span>
              {% endif %}
            </div>
          </div>
        </div>
        {% endfor %}
      </section>
      <section class="experience">
        <h2>Experience</h2>
        {% for w in resume.work %}
        <div class="item">
          <div class="left">
            <div class="company">
              <strong>
                {{ w.company }}
              </strong>
            </div>
            <div class="position">
              {{ w.position }}
            </div>
          </div>
          <div class="right">
            <div class="work-location">
              <strong>
                {{ w.summary }}
              </strong>
            </div>
            <div class="date">
              <span class="startDate">
                {{ w.startDate | date: "%b %Y" }}
              </span>
              {% if w.endDate %}
              <span class="endDate">
                - {{ w.endDate | date: "%b %Y" }}
              </span>
              {% else %}
              <span class="endDate">
                - Present
              </span>
              {% endif %}
            </div>
          </div>
        </div>
        <ul>
          {% for h in w.highlights %}
          <li>{{ h }}</li>
          {% endfor %}
        </ul>
        {% endfor %}
      </section>
      <section class="awards">
        <h2>Awards, Scholarships, &amp; Achievements</h2>
        {% for a in resume.awards %}
        <div class="item">
          <div class="left">
            <div class="title">
              <strong>
                {{ a.title }}
              </strong>
            </div>
            <div class="award-summary">
              {{ a.summary }}
            </div>
          </div>
          <div class="right">
            <div class="awarder">
              <strong>
                {{ a.awarder }}
              </strong>
            </div>
            <div class="date">
              {{ a.date | date: "%b %Y" }}
            </div>
          </div>
        </div>
        {% endfor %}
      </section>
      <section class="interests">
        <h2>Interests</h2>
        {% for i in resume.interests %}
        <div class="interest">
          <strong>{{ i.name }}: </strong>{{ i.keywords | join: ', ' }}
        </div>
        {% endfor %}
      </section>
    </div>
  </body>
</html>
